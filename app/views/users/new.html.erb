<h3>Please fill out the below fields in order to create a user:</h3>

<div class="create_form">
  <%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit %>
  <% end %>
</div>
